<div class="row-fluid">
  <a href="projects/{{ basename }}.html">
  <div class="col-sm-5 col-xs-12 details">
    <h2>{{ data.title }}</h2>
    <h5>
      {{ data.subtitle }}
    </h5>
    <p class="hidden-xs">{{ data.summary }}</p>
    {{#if data.status }}
      <span class="label-trans">{{ data.status }}</span>
    {{/if}}
  </div>
  <div class="col-sm-7 col-xs-12 thumb">
    <div class="img-container">
      <img src="{{assets}}/images/projects/{{basename}}/{{ data.thumb }}">
    </div>
  </div>
  </a>
  <div class="clearfix"></div>
</div>
